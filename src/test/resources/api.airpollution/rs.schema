{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "coord": {
      "type": "object",
      "properties": {
        "lon": {
          "type": "integer"
        },
        "lat": {
          "type": "integer"
        }
      },
      "additionalProperties": true,
      "required": [
        "lon",
        "lat"
      ]
    },
    "list": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "main": {
              "type": "object",
              "properties": {
                "aqi": {
                  "type": "integer"
                }
              },
              "additionalProperties": true,
              "required": [
                "aqi"
              ]
            },
            "components": {
              "type": "object",
              "properties": {
                "co": {
                  "type": "number"
                },
                "no": {
                  "type": "integer"
                },
                "no2": {
                  "type": "number"
                },
                "o3": {
                  "type": "number"
                },
                "so2": {
                  "type": "number"
                },
                "pm2_5": {
                  "type": "number"
                },
                "pm10": {
                  "type": "number"
                },
                "nh3": {
                  "type": "number"
                }
              },
              "additionalProperties": true,
              "required": [
                "co",
                "no",
                "no2",
                "o3",
                "so2",
                "pm2_5",
                "pm10",
                "nh3"
              ]
            },
            "dt": {
              "type": "integer"
            }
          },
          "additionalProperties": true,
          "required": [
            "main",
            "components",
            "dt"
          ]
        }
      ],
      "additionalItems": true
    }
  },
  "additionalProperties": true,
  "required": [
    "coord",
    "list"
  ]
}